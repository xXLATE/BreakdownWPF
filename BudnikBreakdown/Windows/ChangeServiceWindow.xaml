<Window x:Class="BudnikBreakdown.Windows.ChangeServiceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BudnikBreakdown.Windows"
        mc:Ignorable="d"
        Title="Услуга" MinHeight="220" MinWidth="300"
        Height="220" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Дата"/>
        <DatePicker Grid.Row="1" SelectedDate="{Binding StartTime}"/>
        <Button x:Name="btnSave" Grid.Row="2" Content="Сохранить" Click="btnSave_Click"/>
        <Button x:Name="btnDelete" Grid.Row="3" Content="Удалить" Click="btnDelete_Click"/>

    </Grid>
</Window>
